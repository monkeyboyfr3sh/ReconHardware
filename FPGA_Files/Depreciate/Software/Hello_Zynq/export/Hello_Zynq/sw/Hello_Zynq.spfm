<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="Hello_Zynq"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
Hello_Zynq
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="Hello_Zynq">
        <sdx:configuration sdx:name="Hello_Zynq"
                           sdx:displayName="Hello_Zynq"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>Hello_Zynq</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="Hello_Zynq/boot/Hello_Zynq.bif"
                            sdx:readme="Hello_Zynq/boot/generic.readme"
                            sdx:qemuArguments="Hello_Zynq/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="Hello_Zynq/boot/Hello_Zynq.bif"
                            sdx:readme="Hello_Zynq/boot/generic.readme"
                            sdx:qemuArguments="Hello_Zynq/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="Hello_Zynq/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="Hello_Zynq/standalone_domain/bspinclude/include"
                        sdx:bspConfig="Hello_Zynq/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
