<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="data_process_hw"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
data_process_hw
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="data_process_hw">
        <sdx:configuration sdx:name="data_process_hw"
                           sdx:displayName="data_process_hw"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>data_process_hw</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="data_process_hw/boot/data_process_hw.bif"
                            sdx:readme="data_process_hw/boot/generic.readme"
                            sdx:qemuArguments="data_process_hw/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="data_process_hw/boot/data_process_hw.bif"
                            sdx:readme="data_process_hw/boot/generic.readme"
                            sdx:qemuArguments="data_process_hw/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="data_process_hw/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="data_process_hw/standalone_domain/bspinclude/include"
                        sdx:bspConfig="data_process_hw/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
